<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.collegediary.model.user.UserType" table="CLG_DIARY_USER_TYPE">
		<id name="typeId" type="java.lang.Integer">
			<column name="TYPEID" />
		</id>
		<property name="typeDescription" type="java.lang.String">
			<column name="TYPEDESCRIPTION" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
